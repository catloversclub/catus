generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              Int     @id @default(autoincrement())
  kakaoId         String  @unique @map("kakao_id")
  name            String
  phone           String?
  profileImageUrl String? @map("profile_image_url")

  @@map("user")
}